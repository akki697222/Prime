if type(printf) ~= "function" then error("Not Prime") return end
local process = require("system.process")
local printOutput = require("system.util.printOutput")
local printtable = {{"USER", "PID", "STAT", "COMMAND"}}
for index, value in ipairs(process.get()) do
    table.insert(printtable, {value.user, value.PID, coroutine.status(value.thread), value.path})
end
printOutput(printtable)